{
    "name": "tailwindcss-primeui",
    "version": "0.4.0",
    "description": "Utilities for Tailwind CSS",
    "repository": {
        "type": "git",
        "url": "https://github.com/primefaces/tailwindcss-primevue.git"
    },
    "license": "MIT",
    "keywords": [
        "tailwindcss",
        "tailwind",
        "plugin"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "/dist"
    ],
    "scripts": {
        "build": "tsc",
        "watch": "tsc --watch",
        "format": "prettier --write \"**/*.{js,ts}\" --cache",
        "format:check": "prettier --check \"**/*.{js,ts}\"",
        "prepublishOnly": "npm run build"
    },
    "peerDependencies": {
        "tailwindcss": ">=3.1.0"
    },
    "devDependencies": {
        "@types/node": "^20.11.19",
        "@types/tailwindcss": "^3.1.0",
        "prettier": "^2.8.8",
        "tailwindcss": "^3.1.0",
        "typescript": "^5.3.3"
    }
}
